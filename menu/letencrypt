#!/bin/bash

echo "========================================================================="
echo "Quản lý Let's Encrypt"
echo "========================================================================="
PS3='Nhập lựa chọn của bạn (1-4) [0=Thoát]: '
options=("Thêm Let's Encrypt")
select opt in "${options[@]}"
do
    case $opt in
        "Thêm Let's Encrypt")
            '/var/hostvn/script/menu/controller/letencrypt/create'
            ;;
        *) '/var/hostvn/script/menu/hvn';;
    esac
done

